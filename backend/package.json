{
  "name": "backend",
  "version": "1.0.0",
  "description": "School Management System backend (Express + Prisma + PostgreSQL)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "test": "echo \"Add tests\""
  },
  "keywords": [
    "school-management",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "bcrypt": "^6.0.0",
    "csv-parse": "^5.5.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.2",
    "exceljs": "^4.4.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "pg": "^8.16.3",
    "json2csv": "^6.0.0-alpha.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.0.3",
    "@types/multer": "^1.4.12",
    "@types/pg": "^8.16.0",
    "prisma": "^4.16.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
